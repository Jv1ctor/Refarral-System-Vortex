{
  "name": "api",
  "version": "1.0.0",
  "description": "Sistema de indicação para candidatura no Vortex Unifor",
  "main": "index.js",
  "scripts": {
    "prisma:generate": "prisma generate --schema=./src",
    "prisma:migrate:dev": "prisma migrate dev --schema=./src",
    "prisma:studio": "prisma studio --schema=./src",
    "setup": "npm run prisma:generate && npm run prisma:migrate:dev",
    "dev": "tsx watch --env-file=.env ./src/server.ts",
    "build": "tsc -b",
    "check-types": "tsc --noEmit"
  },
